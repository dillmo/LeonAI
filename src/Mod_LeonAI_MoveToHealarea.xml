<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <condition id="isInNamedArea">
                    <string id="area name">areaHome</string>
                    <string id="team" values="ownenemy">ENEMY_TEAM</string>
                    <string id="who" values="targetself">self</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">MoveToHealArea_atEnemyBase</string>
                            <string id="value" values="flagtoggle">yes</string>
                        </action>
                    </normal>
                    <else>
                        <action id="setBool">
                            <string id="id">MoveToHealArea_atEnemyBase</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                    </else>
                </condition>
                <condition id="isInArea">
                    <string id="condition" values="yesno">yes</string>
                    <string id="collision groups" values="collison groups" multiselect="true">NAMED_AREAS;;</string>
                    <string id="check green obstacles only" values="yesno">no</string>
                    <string id="count characters out of combat" values="yesno">yes</string>
                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;</string>
                    <string id="areaName">HEALAREA</string>
                    <float id="xOffset">-0.10</float>
                    <float id="yOffset">0.00</float>
                    <float id="width">0.20</float>
                    <float id="height">0.01</float>
                    <string id="check line of sight" values="yesno">no</string>
                    <string id="ignore invisibility" values="yesno">no</string>
                    <string id="Comment">Already at HEALAREA?</string>
                    <else>
                        <andblock>
                            <string id="Comment">Move towards the enemy base if I am at or near my team&apos;s STARTAREA</string>
                            <normal>
                                <condition id="isFromTeam">
                                    <string id="team" values="teamswithnumbers">ZERO_TEAM</string>
                                    <string id="check on" values="targetself">self</string>
                                    <string id="Comment">Red team moves right, blue team moves left</string>
                                    <normal>
                                        <action id="pressStick">
                                            <string id="direction" values="directions">RIGHT</string>
                                            <float id="duration in seconds">0.10</float>
                                        </action>
                                    </normal>
                                    <else>
                                        <action id="pressStick">
                                            <string id="direction" values="directions">LEFT</string>
                                            <float id="duration in seconds">0.10</float>
                                        </action>
                                    </else>
                                </condition>
                            </normal>
                            <else>
                                <condition id="getBoolEquals">
                                    <string id="id">false</string>
                                    <string id="value" values="yesno">no</string>
                                    <string id="Comment">Move away from enemy base since I&apos;m not at or near my team&apos;s STARTAREA</string>
                                    <normal>
                                        <action id="selectDestWaypoint">
                                            <string id="name">regenHome</string>
                                            <string id="team" values="ownenemy">OWN_TEAM</string>
                                        </action>
                                        <condition id="isNextWaypointPosition">
                                            <string id="position" values="offsetposition">back</string>
                                            <normal>
                                                <action id="pressStick">
                                                    <string id="direction" values="directions">BACKWARD</string>
                                                    <float id="duration in seconds">0.10</float>
                                                </action>
                                            </normal>
                                            <else>
                                                <condition id="isAtDestWaypoint">
                                                    <string id="Comment">Move forward unless I&apos;m where I need to be. Ensures no corner cases are met.</string>
                                                    <normal>
                                                        <condition id="isNextWaypointPosition">
                                                            <string id="position" values="offsetposition">BELOW</string>
                                                            <string id="Comment">Jump if my next waypoint is up.</string>
                                                            <else>
                                                                <condition id="isNextWaypointPosition">
                                                                    <string id="position" values="offsetposition">VERTICAL EQUAL</string>
                                                                    <else>
                                                                        <action id="setBool">
                                                                            <string id="id">jump</string>
                                                                            <string id="value" values="flagtoggle">yes</string>
                                                                        </action>
                                                                    </else>
                                                                </condition>
                                                            </else>
                                                        </condition>
                                                    </normal>
                                                    <else>
                                                        <action id="pressStick">
                                                            <string id="direction" values="directions">FORWARD</string>
                                                            <float id="duration in seconds">0.10</float>
                                                        </action>
                                                    </else>
                                                </condition>
                                            </else>
                                        </condition>
                                        <condition id="isNextWaypointPosition">
                                            <string id="position" values="offsetposition">BELOW</string>
                                            <normal>
                                                <action id="pressButton">
                                                    <string id="button" values="buttons">SHOULDER_LEFT</string>
                                                    <float id="duration in seconds">0.10</float>
                                                </action>
                                            </normal>
                                            <else>
                                                <condition id="isNextWaypointPosition">
                                                    <string id="position" values="offsetposition">FRONT</string>
                                                    <string id="Comment">Jump to avoid obstacles to my front</string>
                                                    <normal>
                                                        <condition id="getBoolEquals">
                                                            <string id="id">CREEPS_UNFRIENDLY_10_10</string>
                                                            <string id="value" values="yesno">yes</string>
                                                            <normal>
                                                                <orblock>
                                                                    <normal>
                                                                        <action id="setBool">
                                                                            <string id="id">jump</string>
                                                                            <string id="value" values="flagtoggle">yes</string>
                                                                        </action>
                                                                    </normal>
                                                                    <or>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">CREEPS_UNFRIENDLY_9_9</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">CREEPS_UNFRIENDLY_9_8</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">CREEPS_UNFRIENDLY_9_7</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                    </or>
                                                                </orblock>
                                                            </normal>
                                                        </condition>
                                                        <condition id="getBoolEquals">
                                                            <string id="id">INVINCIBLE_OBSTACLES_ALL_10_10</string>
                                                            <string id="value" values="yesno">yes</string>
                                                            <normal>
                                                                <orblock>
                                                                    <normal>
                                                                        <action id="setBool">
                                                                            <string id="id">jump</string>
                                                                            <string id="value" values="flagtoggle">yes</string>
                                                                        </action>
                                                                    </normal>
                                                                    <or>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">INVINCIBLE_OBSTACLES_ALL_9_9</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">INVINCIBLE_OBSTACLES_ALL_9_8</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">INVINCIBLE_OBSTACLES_ALL_9_7</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                    </or>
                                                                </orblock>
                                                            </normal>
                                                        </condition>
                                                        <condition id="getBoolEquals">
                                                            <string id="id">PLAYERS_UNFRIENDLY_10_10</string>
                                                            <string id="value" values="yesno">yes</string>
                                                            <normal>
                                                                <orblock>
                                                                    <normal>
                                                                        <action id="setBool">
                                                                            <string id="id">jump</string>
                                                                            <string id="value" values="flagtoggle">yes</string>
                                                                        </action>
                                                                    </normal>
                                                                    <or>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PLAYERS_UNFRIENDLY_9_9</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PLAYERS_UNFRIENDLY_9_8</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PLAYERS_UNFRIENDLY_9_7</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                    </or>
                                                                </orblock>
                                                            </normal>
                                                        </condition>
                                                        <condition id="getBoolEquals">
                                                            <string id="id">PROJECTILES_UNFRIENDLY_10_10</string>
                                                            <string id="value" values="yesno">yes</string>
                                                            <normal>
                                                                <orblock>
                                                                    <normal>
                                                                        <action id="setBool">
                                                                            <string id="id">jump</string>
                                                                            <string id="value" values="flagtoggle">yes</string>
                                                                        </action>
                                                                    </normal>
                                                                    <or>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PROJECTILES_UNFRIENDLY_9_9</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PROJECTILES_UNFRIENDLY_9_8</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                        <condition id="getBoolEquals">
                                                                            <string id="id">PROJECTILES_UNFRIENDLY_9_7</string>
                                                                            <string id="value" values="yesno">yes</string>
                                                                        </condition>
                                                                    </or>
                                                                </orblock>
                                                            </normal>
                                                        </condition>
                                                    </normal>
                                                </condition>
                                            </else>
                                        </condition>
                                    </normal>
                                </condition>
                            </else>
                            <or>
                                <condition id="isInArea">
                                    <string id="condition" values="yesno">yes</string>
                                    <string id="collision groups" values="collison groups" multiselect="true">NAMED_AREAS;;</string>
                                    <string id="check green obstacles only" values="yesno">no</string>
                                    <string id="count characters out of combat" values="yesno">yes</string>
                                    <string id="teams" values="teams" multiselect="true">OWN_TEAM;;</string>
                                    <string id="areaName">STARTAREA</string>
                                    <float id="xOffset">-0.50</float>
                                    <float id="yOffset">0.00</float>
                                    <float id="width">1.00</float>
                                    <float id="height">0.01</float>
                                    <string id="check line of sight" values="yesno">no</string>
                                    <string id="ignore invisibility" values="yesno">no</string>
                                </condition>
                                <condition id="getBoolEquals">
                                    <string id="id">MoveToHealArea_atEnemyBase</string>
                                    <string id="value" values="yesno">no</string>
                                </condition>
                            </or>
                        </andblock>
                    </else>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
